<?xml version="1.0" encoding="utf-8"?>
<package >
  <metadata>
    <id>BarcodeQRCodeSDK</id>
    <version>2.3.4</version>
    <title>Barcode and QR Code Reader SDK</title>
    <authors>yushulx</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <readme>docs\README.md</readme>
    <!-- <icon>icon.png</icon> -->
    <projectUrl>https://github.com/yushulx/dotnet-barcode-qr-code-sdk</projectUrl>
    <description>The Barcode and QR Code Reader SDK is a wrapper for Dynamsoft Barcode Reader SDK, supporting x64 Windows, x64 Linux, x64 macOS, Android and iOS</description>
    <releaseNotes>Added support for Android</releaseNotes>
    <copyright>$copyright$</copyright>
    <tags>Native native Android barcode qrcode Aztec QR Datamatrix PDF417 Code39 Code93 Code128 Codabar Interleaved EAN-8 EAN-13 UPC-A UPC-E GS1Databar PatchCode Maxicode COMPOSITE PostalCode</tags>
    <dependencies>
      <!-- <dependency id="Newtonsoft.Json" version="9.0" /> -->
    </dependencies>
  </metadata>
  <files>
    <file src="README.md" target="docs\" />
    <file src="LICENSE.txt" target=""/>

    <!-- Desktop -->
    <file src="desktop\platform\win\**\*.*" target="runtimes\win-x64\native" />
    <file src="desktop\platform\macos\**\*.*" target="runtimes\osx-x64\native" />
    <file src="desktop\platform\linux\**\*.*" target="runtimes\linux-x64\native" />
    <file src="desktop\bin\Release\net6.0\BarcodeQRCodeSDK.dll" target="lib\net6.0" />
    <file src="desktop\include\**\*.*"   target="build\native\include" />
    <file src="BarcodeQRCodeSDK.targets"   target="build\native" />

    <!-- Android -->
    <file src="android\bin\Release\net6.0-android\**\*.*" target="lib\net6.0-android31.0" />

    <!-- iOS -->
    <file src="ios\bin\Release\net6.0-ios\BarcodeQRCodeSDK.dll" target="lib\net6.0-ios16.1" />
    <file src="ios\bin\Release\net6.0-ios\BarcodeQRCodeSDK.resources\**\*.*" target="lib\net6.0-ios16.1\BarcodeQRCodeSDK.resources" />
  </files>
</package>
